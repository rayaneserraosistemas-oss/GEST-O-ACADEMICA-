{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 --no-open",
    "start:test": "ng serve --port 4200 --no-open",
    "start:dev:all": "ng serve --port 4201 --no-open",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:keycloak": "docker compose up -d keycloak",
    "dev:keycloak:fg": "docker compose up keycloak",
    "dev:back": "mvn -f ./java/pom.xml quarkus:dev",
    "dev:back:wait": "wait-on -t 120000 http-get://localhost:8180/realms/gestao-academica/.well-known/openid-configuration && mvn -f ./java/pom.xml quarkus:dev",
  "dev:all": "concurrently -k -n KEYCLOAK,BACKEND,FRONTEND -c yellow,blue,green \"npm run dev:keycloak:fg\" \"npm run dev:back:wait\" \"npm run start:dev:all\"",
    "e2e": "playwright test",
    "e2e:install": "playwright install --with-deps"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "keycloak-js": "^26.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "@playwright/test": "^1.48.0",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
